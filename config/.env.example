# 环境变量配置模板
# 复制此文件为 .env 并填入实际值

# Alpha Vantage API配置
ALPHA_VANTAGE_API_KEY=your_api_key_here
ALPHA_VANTAGE_BASE_URL=https://www.alphavantage.co/query

# 应用配置
DEBUG=True
LOG_LEVEL=INFO

# 缓存配置（暂时不启用）
CACHE_ENABLED=False
CACHE_TTL=300

# Web服务器配置
HOST=127.0.0.1
PORT=8050

# API请求配置
REQUEST_TIMEOUT=30
MAX_RETRIES=3
RETRY_DELAY=1