# iFinance 应用环境变量配置示例
# 复制此文件为 .env 并填入实际值

# Alpha Vantage API 配置
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# 应用配置
ENVIRONMENT=development
DEBUG=true

# 服务器配置
HOST=127.0.0.1
PORT=8050

# 时区设置
TZ=Asia/Shanghai

# 日志级别
LOG_LEVEL=INFO

# 数据缓存配置
CACHE_TIMEOUT=3600

# API 请求配置
API_TIMEOUT=30
API_RETRY_COUNT=3